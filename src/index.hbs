    <meta property="og:site_name" content="In&amp;Out Centro de Belleza en Málaga">
    <meta name="twitter:card" content="summary">
    <meta id="twitter-meta-description" name="twitter:description"
          content="{{htmlWebpackPlugin.options.description}}">
    <meta id="twitter-meta-title" name="twitter:title" content="{{htmlWebpackPlugin.options.title}}">
    <meta name="twitter:site" content="@bellezainandout">
    <meta name="twitter:image" content="{{htmlWebpackPlugin.options.image}}">
    <meta name="twitter:creator" content="@bellezainandout">

    <!-- Google Analytics -->
    <script>
        window.ga = window.ga || function () {
            (ga.q = ga.q || []).push(arguments);
        };
        ga.l = +new Date;
        ga('create', 'UA-119607581-1', 'auto');
        ga('send', 'pageview');
    </script>
    <script>window.jsEnabled = {{htmlWebpackPlugin.options.isSpider}}</script>
    <script async src='https://www.google-analytics.com/analytics.js'></script>
    <!-- End Google Analytics -->
    <link href="/css/style.css?v={{htmlWebpackPlugin.options.version}}" rel="stylesheet"/>
    <style>
        body {
            background-color: white;
        }

        #boot {
            position: absolute;
            top: 50%;
            left: 50%;
            margin-top: -50px;
            margin-left: -50px;
            width: 100px;
            text-align: center;
            font-family: Arial;
            height: 110px;
            display: none;
        }

        #boot svg {
            animation: loading 2s infinite;
        }

        #system {
            opacity: 0;
        }

        @keyframes loading {
            0% {
                transform: rotateY(0deg)
            }
            100% {
                transform: rotateY(360deg)
            }
        }

        #boot {
        }

        #welcome {
            display: none;
        }
    </style>
    {{htmlWebpackPlugin.options.style}}
    <noscript>
        <style>
            body {
                overflow: auto !important;
            }

            img {
                width: auto !important;
            }

            #static {
                display: block;
            }

            #boot {
                display: none;
            }

            #welcome {
                display: none;
            }
        </style>
    </noscript>
    <title>{{htmlWebpackPlugin.options.title}}</title>
</head>
